{
  "name": "react-carriage-input",
  "description": "A text input component, with the cursor locked in the center",
  "version": "0.0.1",
  "dependencies": {
    "react": "^15.3.1",
    "react-dom": "^15.3.1"
  },
  "devDependencies": {
    "@kadira/storybook": "^2.0.0",
    "eslint": "^3.3.1",
    "eslint-config-standard": "^6.0.0",
    "eslint-config-standard-react": "^3.0.0",
    "eslint-plugin-promise": "^2.0.1",
    "eslint-plugin-react": "^6.1.2",
    "eslint-plugin-standard": "^2.0.0",
    "fixpack": "^2.3.1",
    "flow-bin": "^0.31.1",
    "react-scripts": "^0.2.2"
  },
  "engines": {
    "node": ">=6",
    "npm": ">=3"
  },
  "eslintConfig": {
